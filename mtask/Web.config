<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <typeAliases>
      <typeAlias alias="IUserRepository" type="mtask.Models.Repository.IUserRepository, mtask" />
      <typeAlias alias="UserRepository" type="mtask.Models.Repository.UserRepository, mtask" />
      <typeAlias alias="IUserService" type="mtask.Services.IUserService, mtask" />
      <typeAlias alias="UserService" type="mtask.Services.UserService, mtask" />
      <typeAlias alias="IProjectService" type="mtask.Services.IProjectService, mtask" />
      <typeAlias alias="ProjectService" type="mtask.Services.ProjectService, mtask" />
    </typeAliases>
    <containers>
      <container name="Product">
        <register type="IUserRepository" mapTo="UserRepository">
          <lifetime type="singleton" />
        </register>
        <register type="IUserService" mapTo="UserService">
          <lifetime type="singleton" />
        </register>
        <register type="IProjectService" mapTo="ProjectService">
          <lifetime type="singleton" />
        </register>
      </container>
    </containers>
  </unity>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="StorageConnectionString" value="UseDevelopmentStorage=true" />
    <add key="ApplicationName" value="mtask" />
    <!-- Azure Storage: mtask -->
    <add key="mtask_AzureStorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=mtask;AccountKey=bb2F3hv2Q7hvRBcETumzZR9xiwK95fqydaEU0l5aHFSpNzUcAlzd48u+9OsqJJFfSXl8Y4lGqPSYhO1logOlBw==" />
    <add key="SendGridAPIKey" value=""/>
    <add key="SendGridFrom" value=""/>
    <add key="SendGridFromName" value=""/>
    <add key="SendGridBcc" value=""/>
  </appSettings>
  <!--
    web.config の変更点の説明については、http://go.microsoft.com/fwlink/?LinkId=235367 を参照してください。

    次の属性を <httpRuntime> タグに設定できます。
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="ApplicationInsightsWebTracking" />
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" preCondition="managedHandler" />
    </modules>
  </system.webServer>
  <runtime>
